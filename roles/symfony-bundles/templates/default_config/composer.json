{
    "name": "{{project_name}}/site",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            {% if 'lte-admin' in bundles %}"{{project_name|title}}\\AdminBundle\\": "src/{{project_name|title}}/AdminBundle",{% endif %}

            {% if 'user' in bundles %}"{{project_name|title}}\\UserBundle\\": "src/{{project_name|title}}/UserBundle",{% endif %}

            "{{project_name|title}}\\{{ core_bundle_full_name }}\\": "src/{{project_name|title}}/{{ core_bundle_full_name }}",
            "{{project_name|title}}\\{{ core_bundle_name }}Bundle\\": "src/{{project_name|title}}/{{ core_bundle_name }}Bundle"
        },
        "classmap": [
            "app/AppKernel.php",со
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "avanzu/admin-theme-bundle": "^1.3",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "egeloen/ckeditor-bundle": "^5.0",
        "friendsofsymfony/jsrouting-bundle": "^1.6",
        "friendsofsymfony/rest-bundle": "^2.1",
        "friendsofsymfony/user-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "helios-ag/fm-elfinder-bundle": "~6",
        "hwi/oauth-bundle": "^0.5.3",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/serializer-bundle": "^1.4",
        "jms/translation-bundle": "^1.3",
        "knplabs/knp-menu-bundle": "^2.1",
        "knplabs/knp-paginator-bundle": "^2.6",
        "mopa/bootstrap-bundle": "^3.2",
        "nelmio/api-doc-bundle": "^2.13",
        "ramsey/uuid": "^3.7",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "suncat/mobile-detect-bundle": "1.0.*",
        "symfony-cmf/seo-bundle": "^2.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
